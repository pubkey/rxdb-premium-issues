{
  "name": "rxdb-premium-issues",
  "version": "0.0.0",
  "private": true,
  "description": "Repo to submit bug reports and test cases for the RxDB Premium Plugins",
  "homepage": "https://github.com/pubkey/rxdb-premium-issues#readme",
  "bugs": {
    "url": "https://github.com/pubkey/rxdb-premium-issues/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubkey/rxdb-premium-issues.git"
  },
  "author": "pubkey",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test:browser": "karma start",
    "test:node": "mocha --expose-gc --config ./config/.mocharc.cjs ./bug-report.test.ts",
    "test": "npm run test:node && npm run test:browser"
